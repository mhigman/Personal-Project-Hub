{
  "name": "compliments",
  "displayName": "Compliments",
  "description": "An extension to give compliments to you during coding",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "compliments.helloWorld",
        "title": "Hello World"
      }
    ],
    "configuration": {
      "title": "Compliments",
      "properties": {
        "compliments.listofcompliments": {
          "type": "array",
          "default": [
            "You can do it! :>",
            "Only one debugging session away :)",
            "I believe in you!",
            "39 buried, 0 found =\\"
          ],
          "description": "The list of compliments the extension will give you!"
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.2",
    "@types/node": "18.x",
    "@types/vscode": "^1.83.0",
    "@vscode/test-electron": "^2.3.4",
    "eslint": "^8.50.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2"
  }
}
